        <div>
          <img class="card" id="bottom-right-card" alt="">
        </div>
      </div>

      <form action="process" method="post" enctype="multipart/form-data">
        <input style="display: none;" id="file-input" type="file" accept="image/*" value="" name="image">
      </form>

    </div>
    <div id="samsung-container" class="w3-center">
      <img id="samsung" alt="">
    </div>

    <!-- happy new year application code -->
